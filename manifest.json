{
  "manifest_version": 3,
  "name": "Synced Tabs Viewer",
  "version": "1.0",
  "permissions": ["sessions", "tabs", "storage", "bookmarks"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "page.html"
  }
}
